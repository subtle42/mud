{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./index.ts --inspect=5858",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@types/node": "^16.11.13",
    "@types/yamljs": "^0.2.31",
    "jsonschema": "^1.4.0",
    "redux": "^4.1.2",
    "rfdc": "^1.3.0",
    "socket.io": "^4.4.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/sinon": "^10.0.6",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "sinon": "^12.0.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      ".dist/"
    ],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "ext": "ts"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
